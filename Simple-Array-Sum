import java.util.Scanner;
public class array1{
    public static void main(String args[]){
        Scanner scan = new Scanner(System.in);
        int n=scan.nextInt();                                  //read int input from user
        int sum =0;
        int ar[] = new int [n];                               //initialize array of size n 
        for (int i =0; i<ar.length; i++){                    //arrays have .length property not .length()
            ar[i] = scan.nextInt();                         //take array element at index i: ar[i] and add it to sum (sum=0) later on
            sum = sum + ar[i];
        }
        System.out.println(sum);
    }
}
