import java.util.Scanner;
public class asum{
    public static void main(String args[]){
        Scanner scan = new Scanner(System.in);
        int n = scan.nextInt();
        long sum =0l;
        long ar[] = new long[n];
        for (int i =0; i<ar.length; i++){
            ar[i] = scan.nextLong();
            sum = sum + ar[i];
        }
        System.out.println(sum);
    }
}
